ck=\"thisPage\" class=\"pagination__page-num\" v-for=\"pageNumber in totalPages\" :key=\"pageNumber\"\n      :to=\"getPageLink(pageNumber)\" :class=\"{ 'pagination__page-num_active': сurrentPage === pageNumber }\">\n      {{ pageNumber }}\n    </router-link>\n\n    <button @click=\"nextPage\" :disabled=\"currentPage === totalPages\" class=\"pagination__next-btn\"><svg\n        xmlns=\"http://www.w3.org/2000/svg\" width=\"53\" height=\"52\" viewBox=\"0 0 53 52\" fill=\"none\">\n        <circle cx=\"26.5\" cy=\"26\" r=\"25.5\" stroke=\"#CDA274\" />\n        <path d=\"M23.5571 32L29.5 25.3143L23.5571 18.6286\" stroke=\"#292F36\" stroke-width=\"2\" stroke-linecap=\"round\"\n          stroke-linejoin=\"round\" />\n      </svg></button>\n\n\n  </div>\n</template>\n\n<script>\nimport { mapActions, mapGetters, mapState } from 'vuex'\n\nexport default {\n  props: {\n    msg: String\n  },\n  data() {\n    return {\n    }\n  },\n  computed: {\n    ...mapState(['currentPage']),\n    ...mapGetters(['totalPages']),\n  },\n  methods: {\n    ...mapActions(['setCurrentPage',]),\n    getCurrentPageNumber() {\n      const pageNumberParam = parseInt(this.$route.params.pageNumber)\n      return isNaN(pageNumberParam) || pageNumberParam < 1 ? 1 : pageNumberParam\n    },\n    prevPage() {\n      if (this.getCurrentPageNumber > 1) {\n        this.setCurrentPage(this.getCurrentPageNumber - 1)\n      }\n    },\n    thisPage() {\n      if (this.getCurrentPageNumber === this.totalPages) {\n        this.setCurrentPage(this.getCurrentPageNumber)\n      }\n    },\n    nextPage() {\n      if (this.getCurrentPageNumber < this.totalPages) {\n        this.setCurrentPage(this.getCurrentPageNumber + 1)\n      }\n    },\n    getPageLink(pageNumber) {\n      return `/project/${pageNumber}`;\n    },\n  },\n}\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style scoped lang=\"sass\">\n.pagination\n    display: flex\n    justify-content: center\n    gap: 2rem\n    &__pr